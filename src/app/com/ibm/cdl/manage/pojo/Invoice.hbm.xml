<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ibm.cdl.manage.pojo.Invoice" table="invoice">
    
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid.hex"/>
        </id>
 		<property name="name" type="java.lang.String">
            <column name="NAME"/>
        </property>
 		<property name="createUser" type="java.lang.String">
            <column name="CREATE_USER"/>
        </property>
        <property name="number" type="java.lang.String">
            <column name="NUMBER"/>
        </property>
        <property name="printDate" type="java.lang.String">
            <column name="PRINT_DATE"/>
        </property>
        <property name="invoiceNo" type="java.lang.String">
            <column name="INVOICE_NO"/>
        </property>
        <property name="frameNo" type="java.lang.String">
            <column name="FRAME_NO"/>
        </property>
        <property name="tax" type="java.math.BigDecimal">
            <column name="TAX" />
        </property>
        <property name="createTime" type="java.sql.Timestamp">
            <column name="CREATE_TIME"/>
        </property>
    </class>
</hibernate-mapping>
