<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ibm.cdl.manage.pojo.BusinessLicense" table="business_license">
    
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid.hex"/>
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME"/>
        </property>
 		<property name="creditCode" type="java.lang.String">
            <column name="CREDIT_CODE"/>
        </property>
 		<property name="address" type="java.lang.String">
            <column name="ADDRESS"/>
        </property>
        <property name="regNumber" type="java.lang.String">
            <column name="REG_NUMBER"/>
        </property>
        <property name="incorporator" type="java.lang.String">
            <column name="INCORPORATOR"/>
        </property>
        <property name="endDate" type="java.lang.String">
            <column name="END_DATE"/>
        </property>
        <property name="createUser" type="java.lang.String">
            <column name="CREATE_USER"/>
        </property>
        <property name="createTime" type="java.sql.Timestamp">
            <column name="CREATE_TIME"/>
        </property>
    </class>
</hibernate-mapping>
